generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Food {
  id    Int     @id @default(autoincrement())
  food_name String
  food_calories Int
  food_protein Int
  food_carbohydrate Int
  food_fat Int
  food_ingredients String
  food_recipe String
}